(
    (-71.88 -65.86 1.0)
    (80.16 -65.86 1.0)
    (80.16 67.14 1.0)
    (-71.88 67.14 1.0)
    (-71.88 -65.86 71.5)
    (80.16 -65.86 71.5)
    (80.16 67.14 71.5)
    (-71.88 67.14 71.5)
);
